#include <iostream>
#include <algorithm>
#define  _MAX 999999

using namespace std;

int n;
int edge[16][16];
int cache[65536][16];
//cache는 현재 노드에서 0011...에서 나머지를 갔을 때 최소 값

//0번노드를 갔으면 000...001
//1번노드를 갔으면 000...011

int TSP(int visit , int curnode)
{
	if (visit == (1 << n) - 1)
	{
		return edge[curnode][0];
	}
	
	if (cache[visit][curnode] != 0)
	{
		return cache[visit][curnode];
	}

	int ret = _MAX;
	int temp;

	for (int i = 0; i < n; i++)
	{
		if ((visit & (1 << i)) == 0)
		{
			temp = 1 << i;
			ret = min(ret, TSP(visit + temp , i) + edge[curnode][i]);
		}	
		cache[visit][curnode] =  ret;
	}

	return ret;
}

int main()
{
	int temp=1;
	cin >> n;
	for (int i = 0; i < n; i++)
	{
		for (int j = 0; j < n; j++)
			cin >> edge[i][j];
	}
	cout << TSP(1, 0);

	return 0;
}
